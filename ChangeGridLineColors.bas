Attribute VB_Name = "ChangeGridLineColors"
Option Explicit

Sub GridLineColorAutomatic()
    ActiveWindow.GridlineColorIndex = 0
End Sub

Sub GridLineColorBlack()
    ActiveWindow.GridlineColorIndex = 1
End Sub

Sub GridLineColorRed()
    ActiveWindow.GridlineColorIndex = 3
End Sub

Sub GridLineColorGreen()
    ActiveWindow.GridlineColorIndex = 10
End Sub

